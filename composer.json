{
	"name": "marshmallow/nova-charcounted-fields",
	"description": "Text and textarea fields with a character counter.",
	"keywords": [
		"laravel",
		"nova",
		"character counter"
	],
	"license": "MIT",
	"require": {
		"php": ">=7.1.0"
	},
	"autoload": {
		"psr-4": {
			"Marshmallow\\CharcountedFields\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Marshmallow\\CharcountedFields\\FieldServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
